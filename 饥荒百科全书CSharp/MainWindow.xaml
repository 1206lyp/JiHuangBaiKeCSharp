<Window x:Name="mainWindow" x:Class="饥荒百科全书CSharp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:饥荒百科全书CSharp"
        xmlns:uc="clr-namespace:饥荒百科全书CSharp.MyUserControl"
        mc:Ignorable="d"
        Title="饥荒百科全书" Icon="Resources/DST.ico" 
        Height="660" Width="1000" 
        MinWidth="900" MinHeight="660" 
        MouseLeftButtonDown="Window_MouseLeftButtonDown" 
        WindowStartupLocation="CenterScreen" 
        SizeChanged="mainWindow_SizeChanged" 
        Loaded="mainWindow_Loaded" 
        Style="{DynamicResource WindowStyle}" 
        Cursor="{DynamicResource Cursor_pointer}" 
        WindowStyle="None" 
        AllowsTransparency="True" 
        ResizeMode="CanResizeWithGrip">

    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary/WindowDictionary.xaml" />
                <ResourceDictionary Source="Dictionary/CursorDictionary.xaml" />
                <ResourceDictionary Source="Dictionary/ComboBoxDictionary.xaml" />
                <ResourceDictionary Source="Dictionary/ScrollViewerDictionary.xaml" />
                <ResourceDictionary Source="Dictionary/SliderDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>

    <Border x:Name="MainBorder" BorderThickness="1" BorderBrush="#FF2B2B2B">
        <Grid x:Name="MainGrid" Grid.Column="1" Canvas.Right="0" Background="White">
            <Grid.RowDefinitions>
                <RowDefinition Height="50"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition x:Name="LCWidth" Width="50" MinWidth="50" MaxWidth="150"/>
                <ColumnDefinition x:Name="RWidth" Width="*" />
            </Grid.ColumnDefinitions>
            <Border x:Name="UI_BackGroundBorder" BorderThickness="0" Grid.RowSpan="2" Grid.ColumnSpan="2" Grid.Column="1"/>
            <TextBlock x:Name="UI_Title" Text="饥荒百科全书" FontSize="20" Margin="10,10,0,0" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" />
            <Button x:Name="UI_btn_close" Width="20" ToolTip="退出" Click="UI_btn_close_Click" Height="20" HorizontalAlignment="Right" Grid.Column="2" Margin="0,-10,10,0" Cursor="{DynamicResource Cursor_link}" Style="{DynamicResource UI_btn_close_Style}" Focusable="False" />
            <Button x:Name="UI_btn_maximized" Width="20" ToolTip="最大化" Click="UI_btn_maximized_Click" Height="20" HorizontalAlignment="Right" Grid.Column="2" Margin="0,-10,35,0" Cursor="{DynamicResource Cursor_link}" Style="{DynamicResource UI_btn_maximized_Style}" Focusable="False" />
            <Button x:Name="UI_btn_normal" Width="20" ToolTip="还原" Click="UI_btn_normal_Click" Height="20" HorizontalAlignment="Right" Grid.Column="2" Margin="0,-10,35,0" Visibility="Collapsed" Cursor="{DynamicResource Cursor_link}" Style="{DynamicResource UI_btn_normal_Style}" Focusable="False" />
            <Button x:Name="UI_btn_minimized" Width="20" ToolTip="最小化" Click="UI_btn_minimized_Click" Height="20" HorizontalAlignment="Right" Grid.Column="2" Margin="0,-10,60,0" Cursor="{DynamicResource Cursor_link}" Style="{DynamicResource UI_btn_minimized_Style}" Focusable="False" />
            <Button x:Name="UI_btn_bg" Content="背景设置" HorizontalAlignment="Right" Click="UI_btn_bg_Click" Width="20" Grid.Column="1" Margin="0,-10,85,0" Height="20" Cursor="{DynamicResource Cursor_link}" Style="{DynamicResource UI_btn_bg_Style}" Focusable="False" />
            <Popup x:Name="UI_pop_bg" PopupAnimation="Fade" Width="200" Height="200" PlacementTarget="{Binding ElementName=UI_btn_bg}" Placement="Top" AllowsTransparency="True" StaysOpen="False" IsOpen="False">
                <Border BorderThickness="5" BorderBrush="#FF1566B8" Background="White"  Cursor="{DynamicResource Cursor_pointer}">
                    <StackPanel Margin="10">
                        <Button x:Name="Se_button_Background" Click="Se_button_Background_Click" Background="{x:Null}" BorderBrush="{x:Null}" Height="40" Cursor="{DynamicResource Cursor_link}">
                            <Canvas Height="40" Width="170" Margin="-2,-2,0,0" >
                                <Image Height="40" Width="40" Source="/Resources/Setting_Skin.png" Margin="0,0,0,0"/>
                                <TextBlock TextWrapping="Wrap" Text="选择背景" Canvas.Left="40" Padding="0,12,0,0" Height="40" Width="130" TextAlignment="Center"/>
                            </Canvas>
                        </Button>
                        <Button x:Name="Se_button_Background_Clear" Click="Se_button_Background_Clear_Click" Background="{x:Null}" BorderBrush="{x:Null}" Height="40" Cursor="{DynamicResource Cursor_link}">
                            <Canvas Height="40" Width="170" Margin="-2,-2,0,0" >
                                <Image Height="40" Width="40" Source="Resources/Setting_Skin_Clear.png" Margin="0,0,0,0"/>
                                <TextBlock TextWrapping="Wrap" Text="清除背景" Canvas.Left="40" Padding="0,12,0,0" Height="40" Width="130" TextAlignment="Center"/>
                            </Canvas>
                        </Button>
                        <TextBlock x:Name="Se_BG_Alpha_Text" TextWrapping="Wrap" Text="背景不透明度：100%" Margin="0,10,0,0"/>
                        <Slider x:Name="Se_BG_Alpha" ValueChanged="Se_BG_Alpha_ValueChanged" Style="{DynamicResource SliderStyle}" Maximum="100" SmallChange="1" LargeChange="10" Value="100" Cursor="{DynamicResource Cursor_link}"/>
                        <TextBlock x:Name="Se_Panel_Alpha_Text" TextWrapping="Wrap" Text="面板不透明度：100%" Margin="0,10,0,0"/>
                        <Slider x:Name="Se_Panel_Alpha" ValueChanged="Se_Panel_Alpha_ValueChanged" Style="{DynamicResource SliderStyle}" Maximum="100" SmallChange="1" LargeChange="10" Cursor="{DynamicResource Cursor_link}" />
                    </StackPanel>
                </Border>
            </Popup>
            <Button x:Name="UI_btn_setting" Content="设置" HorizontalAlignment="Right" Click="UI_btn_setting_Click" Width="20" Grid.Column="1" Margin="0,-10,110,0" Height="20" Cursor="{DynamicResource Cursor_link}" Style="{DynamicResource UI_btn_setting_Style}" Focusable="False" />
            <Popup x:Name="UI_pop_setting" PopupAnimation="Fade" Width="200" Height="200" PlacementTarget="{Binding ElementName=UI_btn_setting}" Placement="Top" AllowsTransparency="True" StaysOpen="False" IsOpen="False" Grid.ColumnSpan="2" Margin="0,0,798,458" Grid.RowSpan="2">
                <Border BorderThickness="5" BorderBrush="#FF1566B8" Background="White"  Cursor="{DynamicResource Cursor_pointer}">
                    <StackPanel Margin="10">
                        <Button x:Name="Se_button_Update" Background="{x:Null}" BorderBrush="{x:Null}" Height="40" Cursor="{DynamicResource Cursor_link}" Click="Se_button_Update_Click">
                            <Canvas Height="40" Width="170" Margin="-2,-2,0,0" >
                                <Image Height="40" Width="40" Source="Resources/Setting_Update.png" Margin="0,0,0,0"/>
                                <TextBlock TextWrapping="Wrap" Text="检查更新" Canvas.Left="40" Padding="0,12,0,0" Height="40" Width="130" TextAlignment="Center"/>
                            </Canvas>
                        </Button>
                    </StackPanel>
                </Border>
            </Popup>
            <ComboBox x:Name="UI_gameversion" HorizontalAlignment="Right" VerticalAlignment="Top" Width="100" Margin="0,3,140,0" Cursor="{DynamicResource Cursor_link}" Grid.Column="1" Style="{DynamicResource ComboBoxStyle}" Focusable="False">
                <ComboBoxItem Cursor="{DynamicResource Cursor_link}" IsSelected="True" Focusable="False">
                    <StackPanel Orientation="Horizontal">
                        <Image Source="/Resources/DLC_DS.PNG" Width="30" Height="30" />
                        <TextBlock Text="DS" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </ComboBoxItem>
                <ComboBoxItem Cursor="{DynamicResource Cursor_link}" Focusable="False">
                    <StackPanel Orientation="Horizontal">
                        <Image Source="/Resources/DLC_ROG.PNG" Width="30" Height="30" />
                        <TextBlock Text="RoG" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </ComboBoxItem>
                <ComboBoxItem Cursor="{DynamicResource Cursor_link}" Focusable="False">
                    <StackPanel Orientation="Horizontal">
                        <Image Source="/Resources/DLC_SW.PNG" Width="30" Height="30" />
                        <TextBlock Text="SW" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </ComboBoxItem>
                <ComboBoxItem Cursor="{DynamicResource Cursor_link}" Focusable="False">
                    <StackPanel Orientation="Horizontal">
                        <Image Source="/Resources/DLC_DST.PNG" Width="30" Height="30" />
                        <TextBlock Text="DST" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </ComboBoxItem>
                <ComboBoxItem Cursor="{DynamicResource Cursor_link}" Focusable="False">
                    <StackPanel Orientation="Horizontal">
                        <Image Source="/Resources/DLC_DST_TX.PNG" Width="30" Height="30" />
                        <TextBlock Text="Tencent" VerticalAlignment="Center" Margin="10,0,0,0" />
                    </StackPanel>
                </ComboBoxItem>
            </ComboBox>
            <TextBlock x:Name="UI_gameversion_TextBlock" TextWrapping="Wrap" Text="游戏版本：" Margin="0,13,245,0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Top"/>
            <TextBox x:Name="UI_search" HorizontalAlignment="Right" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="200" Grid.Column="1" Margin="0,10,330,0" Cursor="{DynamicResource Cursor_text}"  Style="{DynamicResource UI_search_Style}" TextChanged="UI_search_TextChanged" FontSize="15"/>
            <Button x:Name="UI_search_clear" Width="20" ToolTip="清除" Height="20" HorizontalAlignment="Right" Grid.Column="1" Margin="0,12,331,20" Cursor="{DynamicResource Cursor_link}" Style="{DynamicResource UI_btn_close_Style}" Click="UI_search_clear_Click" Visibility="Collapsed" Focusable="False"/>
