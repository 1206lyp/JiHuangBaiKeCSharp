                    <ColumnDefinition x:Name="SRWidth" Width="*" MinWidth="100"/>
                </Grid.ColumnDefinitions>
                <GridSplitter x:Name="UI_Splitter" HorizontalAlignment="Left" Height="608" VerticalAlignment="Top" Width="5" Background="#FF1A348F" Cursor="{DynamicResource Cursor_horz}" Grid.Column="1" Visibility="Collapsed"/>
