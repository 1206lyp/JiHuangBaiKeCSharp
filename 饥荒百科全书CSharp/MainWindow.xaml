                    <Style TargetType="{x:Type Button}">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="{x:Type Button}">
                                    <Image x:Name="btnbg_maximized" Source="Resources/Maximized_1.png" />
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Source"  Value="/Resources/Maximized_2.png" TargetName="btnbg_maximized" />
                                        </Trigger>
                                        <Trigger Property="IsPressed" Value="True">
                                            <Setter Property="Source" Value="/Resources/Maximized_3.png" TargetName="btnbg_maximized" />
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Button.Style>
            </Button>
            <Button x:Name="UI_btn_normal" Width="15" ToolTip="还原" Click="UI_btn_normal_Click" Height="15" HorizontalAlignment="Right" Focusable="False" Grid.Column="2" Margin="0,-10,30,0" Visibility="Collapsed" Cursor="{DynamicResource Cursor_link}">
