                    <Style TargetType="{x:Type Button}">
                        <Setter Property="Template">
                            <Setter.Value>
                                <ControlTemplate TargetType="{x:Type Button}">
                                    <Image x:Name="btnbg_close" Source="/Resources/close_1.png" />
                                    <ControlTemplate.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Source"  Value="/Resources/close_2.png" TargetName="btnbg_close" />
                                        </Trigger>
                                        <Trigger Property="IsPressed" Value="True">
                                            <Setter Property="Source" Value="/Resources/close_3.png" TargetName="btnbg_close" />
                                        </Trigger>
                                    </ControlTemplate.Triggers>
                                </ControlTemplate>
                            </Setter.Value>
                        </Setter>
                    </Style>
                </Button.Style>
            </Button>
            <Button x:Name="UI_btn_maximized" Width="15" ToolTip="最大化" Click="UI_btn_maximized_Click" Height="15" HorizontalAlignment="Right" Focusable="False" Grid.Column="2" Margin="0,-10,30,0" Cursor="{DynamicResource Cursor_link}">
